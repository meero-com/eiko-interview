{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "default": {
      "format": "[%(asctime)s] %(levelname)s %(name)-20s %(message)s",
      "datefmt": "%Y-%m-%dT%H:%M:%S%z"
    }
  },
  "handlers": {
    "null": {
      "level": "DEBUG",
      "class": "logging.NullHandler"
    },
    "console": {
      "level": "DEBUG",
      "class": "logging.StreamHandler",
      "formatter": "default"
    }
  },
  "loggers": {
    "uvicorn": {
      "level": "DEBUG"
    },
    "mini_eiko": {
      "level": "DEBUG"
    },
    "": {
      "handlers": [
        "console"
      ],
      "level": "DEBUG"
    }
  }
}